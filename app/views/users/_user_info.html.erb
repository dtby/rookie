<div class="user-info">
	<div class="user-row top-none row">
		<div class="col-xs-12">昵称：<%= current_user.nickname %></div>
	</div>
	<div class="user-row row">
		<div class="col-xs-6 pr-0">性别：<%= current_user.gender %></div>
		<div class="col-xs-6 pr-0">生日：</div>
	</div>
	<div class="user-row row">
		<div class="col-xs-6 pr-0">年龄：</div>
		<div class="col-xs-6 pr-0">星座：</div>
	</div>
	<div class="user-row row">
		<div class="col-xs-6 pr-0">民族：</div>
		<div class="col-xs-6 pr-0">籍贯：</div>
	</div>
	<div class="user-row row">
		<div class="col-xs-6 pr-0">现居地：</div>
		<div class="col-xs-6 pr-0">户口地：上海-上海</div>
	</div>
	<div class="user-row bottom-none row">
		<div class="col-xs-6 pr-0">个人信息完善度：80%</div>
		<div class="col-xs-6 pr-0">
			<button class="btn btn-white user-btn">完善资料</button>
		</div>
	</div>

	<div class="generate-resume">
		<%= link_to("一键生成简历", "#", class: "btn btn-blue btn-block" ) %>
	</div>
</div>
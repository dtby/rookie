<div class="lg-box animated">
  <div class="row">
    <!-- 添加试题表单 -->
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5>用户列表</h5>
          <div class="ibox-tools">
            <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
          </div>
        </div>
        <div class="ibox-content">
          <div class="table-responsive">
            <!-- start列表 -->
            <table class="table table-striped table-condensed">
              <thead>
                <tr class="tests-table">
                  <th>序号</th>
                  <th>编号</th>
                  <th>昵称</th>
                  <th>真实姓名</th> 
                  <th>国家</th>
                  <th>性别</th>
                  <th>手机号</th>
                  <th>邮箱</th>
                  <th>出生地</th>
                  <th>现居地</th>
                  <th>生日</th>
                  <th>星座</th>
                  <th>权限类别</th> 
                  <th>操作</th> 
                </tr>
              </thead>
              <tbody>
                <%= render @users %>
              </tbody>
            </table>
            <h3 class="text-center"><%= paginate @users %></h3>  
          </div><!-- end列表 -->
        </div>
      </div>
    </div>
  </div>
</div>
<div id="usersModal"></div>